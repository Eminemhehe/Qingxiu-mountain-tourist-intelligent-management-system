<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="entity">
  <class name="Visitor" table="visitor">
    <id column="id" name="id">
      <generator class="native"/>
    </id>
    <property column="name" name="name"/>
    <property column="idCardNum" name="idCardNum"/>
    <property column="telNum" name="telNum"/>
    <property column="ticketCardBalance" name="ticketCardBalance"/>
    <property column="ticketType" name="ticketType"/>
    <property column="visitPlace" name="visitPlace"/>
    <property column="visitDate" name="visitDate"/>
    <property column="reamrks" name="reamrks"/>
  </class>
</hibernate-mapping>
